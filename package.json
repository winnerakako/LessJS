{
  "name": "appName",
  "version": "1.0.0",
  "description": "",
  "main": "less-server.js",
  "scripts": {
    "_comment_build": "=== BUILD & COMPILATION ===",
    "build": "npm run clean && tsc -p tsconfig.json && node .less-build-script.js",
    "build:prod": "npm run clean && tsc -p tsconfig.json --sourceMap false && node .less-build-script.js",
    "clean": "rimraf dist",
    "_comment_server": "=== SERVER STARTUP ===",
    "start": "cross-env NODE_ENV=production node -r module-alias/register dist/less-server.js",
    "start:staging": "cross-env NODE_ENV=staging node -r module-alias/register dist/less-server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec node --require tsconfig-paths/register --require @swc-node/register less-server.ts",
    "dev": "npm run start:dev",
    "_comment_quality": "=== CODE QUALITY & VALIDATION ===",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\" --ignore-path .gitignore",
    "lint:staged": "lint-staged",
    "_comment_other": "=== TESTING, SETUP & DEPLOYMENT ===",
    "test": "echo 'Add your test runner here'",
    "prepare": "husky",
    "heroku-postbuild": "npm run build:prod",
    "docker:build": "docker build -t lessjs-app .",
    "docker:run": "docker run -p 8000:8000 lessjs-app"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "_lessjs": "./dist/_lessjs",
    "src": "./dist/src"
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.1",
    "commander": "^13.1.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.7.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "nodemailer-sendgrid": "^1.0.3",
    "undici": "^7.8.0",
    "uuid": "^11.1.0",
    "validator": "^13.12.0",
    "xlsx": "^0.18.5",
    "xss": "^1.0.15"
  },
  "engines": {
    "node": "20.x"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "^1.11.20",
    "@swc/register": "^0.1.10",
    "@types/axios": "^0.9.36",
